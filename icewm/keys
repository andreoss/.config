key "Super+c"           icesh -f center
key "Super+r"           icesh -f restore
key "Super+Down"        icesh -f restore
key "Super+Right"       icesh -f restore vertical top right
key "Super+Left"        icesh -f restore vertical top left
key "Super+Up"          icesh -f maximize
key "Super+Shift+Left"  icesh -f vertical center
key "Super+Home"        icesh -f sizeto 49% 49% top left
key "Super+Prior"       icesh -f sizeto 49% 49% top right
key "Super+Next"        icesh -f sizeto 49% 49% bottom right
key "Super+End"         icesh -f sizeto 49% 49% bottom left
key "Super+Begin"       icesh -f sizeto 49% 49% center
key "Super+slash"       icesh -f windowlist
