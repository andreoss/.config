[alias]
    au  = add --all
    ai  = add -i
    ap  = apply
    as  = apply --stat
    ac  = apply --check
    bc  = rev-parse --abbrev-ref HEAD
    bu  = !git rev-parse --abbrev-ref --symbolic-full-name @{u}
    ci  = commit
    cia = commit --amend
    cm  = commit -m
    cc  = clone
    cc1 = clone --depth 1
    cp = cherry-pick
    last = log -1 HEAD
    d = diff
    dp = diff --patience
    dc = diff --cached
    dk = diff --check
    dck = diff --cached --check
    dt = difftool
    dct = difftool --cached
    fe = fetch
    fo = fetch origin
    fu = fetch upstream
    fp = format-patch
    g  = grep -p
    ll = log --oneline
    lg = log --oneline --graph --decorate
    lp = log -p
    ls = ls-files
    lsf = !git ls-files | grep --color -i
    me  = merge
    co  = checkout
    com = checkout master
    ps  = push
    pu  = pull
    pure = pull --rebase
    rb  = rebase
    ra  = rebase --abort
    rc  = rebase --continue
    ri  = rebase --interactive
    rs  = rebase --skip
    re  = reset
    xx  = reset HEAD
    st  = status -s -b
    sa  = stash apply
    sc  = stash clear
    sd  = stash drop
    sl  = stash list
    sw  = stash show
